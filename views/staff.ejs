<!--Partials at Top-->

  <% include partials/header %>
  <% include partials/nav %>

<div class="container">
<!-- Body-->
    <div class="row with-margin">
    </div>
    <div class="row with-margin">
        <div class="col-lg-8">
            <h3>View All Staff</h3>

        </div>
        <div class="col-lg-4">
            <a class="btn btn-primary float-right" href="/staff/new">Add New Staff</a>
        </div>
    </div>
    <table class="table table-striped">
        <thead>
            <tr>
                <th scope="col">Staff ID#</th>
                <th scope="col">First Name</th>
                <th scope="col">Last Name</th>
            </tr>
        </thead>
        <tbody>
            <!-- dynamically build table DOM based on dbs results -->
                    <% listResults.forEach((staff, index) => { %>
                <tr>
                    <th scope="row"><%= staff.staff_id %></th>
                    <td><%= staff.f_name %></td>
                    <td><%= staff.l_name %></td>
                </tr>
                <% }) %>

        </tbody>
    </table>
</div>

<!--Footer Section Here-->
<% include partials/footer %>


